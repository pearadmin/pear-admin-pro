<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.pearadmin.pro.modules.not.repository.SysRemindTemplateRepository">

    <select id="selectRemindTemplate" resultType="SysRemindTemplate">
        select
        srt.*,
        su.nickname as createName
        from sys_remind_template srt
        left join sys_user su on su.id = srt.create_by
    </select>

</mapper>